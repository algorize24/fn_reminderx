{
  "expo": {
    "name": "reminderx",
    "slug": "reminderx",
    "version": "1.2.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#1E1E1E",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1E",
      "duration": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.siuzy.reminderx",
      "notification": {
        "ios": {
          "sound": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "sound": true
      },
      "package": "com.siuzy.reminderx",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-font"],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1E1E1E",
          "defaultChannel": "default",
          "sounds": ["", ""],
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3887c08e-75b7-42b1-978e-475bd64ce31a"
      }
    },
    "owner": "siuzy"
  }
}
